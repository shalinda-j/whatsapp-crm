{
    "key": "",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "description": "WhatsApp CRM \u00e9 uma extens\u00e3o que adiciona novas funcionalidades poderosas ao WhatsApp Web!",
    "version": "2.2.5",
    "manifest_version": 3,
    "name": "WhatsApp CRM",
    "short_name": "WhatsApp CRM",
    "background": {
        "service_worker": "background.bundle.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage",
        "notifications",
        "identity",
        "identity.email",
        "management",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": [
        "*://*.whatsapp.com/*",
        "*://whatsup.plus/*",
        "*://*.whatsapp.net/*",
        "*://127.0.0.1/*",
        "*://localhost/*",
        "*://app.lotsofwms.in/*",
        "*://api.moretus.click/*",
        "*://st.moretus.click/*",
        "*://wp.moretus.click/*"
    ],
    "action": {
        "default_icon": "icon-34.png"
    },
    "chrome_url_overrides": {},
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.whatsapp.com/*"
            ],
            "js": [
                "./images/bootstrap.js",
                "contentScript.bundle.js",
                "./images/custom.js"
            ],
            "css": [
                "content.styles.css",
                "./images/bootstrap.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icon-128.png",
                "icon-48.png",
                "icon-16.png",
                "icon-34.png",
                "images/*",
                "content.styles.css",
                "inject.styles.css",
                "injectScript.bundle.js",
                "contact_upload.csv",
                "contact_upload.xlsx"
            ],
            "matches": [
                "*://*.whatsapp.com/*"
            ]
        }
    ],
    "oauth2": {
        "client_id": "75391543194-ranjnqpeulunv3bpuah4ct0urrov2qq1.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/contacts",
            "https://www.googleapis.com/auth/userinfo.profile",
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/calendar",
            "https://www.googleapis.com/auth/calendar.events"
        ]
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'; connect-src 'self' data: blob: https://*.whatsapp.net https://www.facebook.com https://crashlogs.whatsapp.net/wa_clb_data https://crashlogs.whatsapp.net/wa_fls_upload_check https://*.google-analytics.com https://analytics.google.com https://wforw.plus wss://*.web.whatsapp.com wss://web.whatsapp.com:5222 wss://web.whatsapp.com https://www.whatsapp.com https://dyn.web.whatsapp.com https://graph.whatsapp.com/graphql/ https://graph.facebook.com/graphql https://*.tenor.co https://*.giphy.com https://whatsup.plus https://*.whatsup.plus http://localhost https://localhost http://127.0.0.1 https://127.0.0.1; frame-src 'self' https://*.whatsapp.com https://web.whatsapp.com; img-src 'self' data: blob: https://* https://*.whatsapp.net; style-src 'self' 'unsafe-inline' https://*; font-src 'self' data: https://*;"
    },
    "cross_origin_embedder_policy": {
        "value": "require-corp"
    },
    "cross_origin_opener_policy": {
        "value": "same-origin"
    }
}